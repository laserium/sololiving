<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sololiving.home.mapper.UserMapper">
    <insert id="save">
        INSERT INTO `USER` (user_id, user_pwd)
        VALUES (#{user.userId}, #{user.userPwd})
    </insert>
</mapper>
